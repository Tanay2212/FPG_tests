<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <menuitem id="service_menu_root"
        name="FPG"
        web_icon="services,static/description/icon.png"
        active="True"
        sequence="30">

        <!-- Added new menus as Sale Order. #CodingBanna -->
        <menuitem id="service_order_menu"
            name="Orders"
            sequence="10">

            <menuitem id="menu_sale_quotations"
                action="action_quotations_with_onboarding"
                groups="sales_team.group_sale_salesman"
                sequence="10"/>

            <menuitem id="menu_service_order"
                name="Orders"
                action="action_service_orders"
                groups="sales_team.group_sale_salesman"
                sequence="20"/>

            <menuitem id="report_service_team"
                name="Services Teams"
                action="sales_team.crm_team_action_sales"
                groups="sales_team.group_sale_manager"
                sequence="30"/>

            <menuitem id="res_partner_menu"
                action="account.res_partner_action_customer"
                groups="sales_team.group_sale_salesman"
                sequence="40"/>

        </menuitem>

        <menuitem id="menu_service_invoicing"
            name="To Invoice"
            groups="sales_team.group_sale_salesman"
            sequence="20">

            <menuitem id="menu_sale_order_invoice"
                action="action_orders_to_invoice"
                sequence="10"/>

            <menuitem id="menu_sale_order_upselling"
                action="action_orders_upselling"
                sequence="20"/>

        </menuitem>

        <menuitem id="product_menu_catalog"
            name="Products"
            sequence="30">

            <menuitem id="menu_product_template_action"
                action="product_template_action"
                sequence="10"/>
            <menuitem id="menu_products"
                action="product.product_normal_action_sell"
                groups="product.group_product_variant"
                sequence="20"/>
            <menuitem id="menu_product_pricelist_main"
                name="Pricelists"
                action="product.product_pricelist_action2"
                groups="product.group_product_pricelist"
                sequence="30"/>

        </menuitem>

        <menuitem id="menu_equipments"
            name="Equipments"
            action="action_equipments"
            sequence="40"/>

        <menuitem id="menu_service_config"
            name="Configuration"
            sequence="50">

            <menuitem id="menu_sales_config"
                sequence="30"
                name="Service Orders">

                <menuitem id="service_order_template_menu"
                    name="Quotation Templates"
                    action="service_order_template_action"
                    sequence="1"/>

                <menuitem id="menu_tag_config"
                    name="Tags"
                    action="sales_team.sales_team_crm_tag_action"
                    sequence="2"/>

            </menuitem>
        </menuitem>
    </menuitem>
</odoo>
